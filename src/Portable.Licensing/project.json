{
	"version": "1.1.1",
	"authors": [
		"Nauck IT KG"
	],
	"buildOptions": {
		"warningsAsErrors": true,
		"compile": {
			"includeFiles": [
				"../CommonAssemblyInfo.cs"
			],
			"include": [
				"*.cs",
				"Properties/*.cs",
				"Security/**/*.cs",
				"Validation/*.cs"
			]
		}
	},
	"dependencies": {
		"Portable.BouncyCastle": "1.8.1.*"
	},
	"frameworks": {
		"net451": {
			"frameworkAssemblies": {
				"System.Xml": "4.0.0",
				"System.Xml.Linq": "4.0.0",
				"System.IO": {
					"type": "build"
				},
				"System.Runtime": {
					"type": "build"
				},
				"System.Runtime.Extensions": {
					"type": "build"
				}
			}
		},
		"netstandard1.0": {
			"buildOptions": {
				"define": [
					"CORECLR"
				]
			},
			"dependencies": {
				"System.Runtime": "4.1.0",
				"System.Runtime.Extensions": "4.1.0",
				"System.Xml.ReaderWriter": "4.0.11",
				"System.Xml.XDocument": "4.0.11"
			}
		}
	}
}